{
  "name": "server",
  "version": "1.0.0",
  "description": "backend server for Proficiency",
  "main": "server.ts",
  "scripts": {
    "dev": "concurrently \"cd client && npm run dev\" \"nodemon --exec ts-node server.ts\"",
